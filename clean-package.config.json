{
  "replace": {
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "require": "./dist/index.js",
        "import": "./dist/index.mjs"
      },
      "./umd": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.js"
      },
      "./package.json": "./package.json"
    }
  },
  "remove": ["clean-package"]
}